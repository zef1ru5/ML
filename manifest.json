{
  "manifest_version": 2,
  "name": "extractionML",
  "description": "Data Extraction from Web-pages with Mashine Learning",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon-16-off.png",
    "48": "icons/icon-64.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16-off.png",
      "24": "icons/icon-24.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "Popup Extractor",
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "gostCrypto/gostEngine.dist.js",
        "gostCrypto/gostCrypto.dist.js",
        "content.js"
      ],
      "css": ["style/class.css"]
    }
  ],  
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "storage",
    "http://server.zef1ru5.keenetic.link/"
  ]
}
